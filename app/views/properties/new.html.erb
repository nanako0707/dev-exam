<h1>物件登録</h1>
<%= form_with(model: @property, local: true) do |form| %>
  <div class="property_name">
    <%= form.label :物件名 %>
    <%= form.text_field :property_name %>
  </div>
  <div class="rent">
    <%= form.label :賃料 %>
    <%= form.text_field :rent %>円
  </div>
  <div class="street_address">
    <%= form.label :住所 %>
    <%= form.text_field :street_address%>
  </div>
  <div class="age">
    <%= form.label :築年数 %>
    <%= form.text_field :age%>年
  </div>
  <div class="note">
    <%= form.label :備考 %>
    <%= form.text_area :note %>
  </div>
    <%= form.submit %>
<% end %>
